{

  "manifest_version": 2,
  "name": "Mulan",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/sun1.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["wisdom.js"]
    }
  ]

}
